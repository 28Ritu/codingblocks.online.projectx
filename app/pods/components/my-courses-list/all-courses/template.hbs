{{#each @runs as |run|}}
  <MyCoursesList::RunRow
    @run={{run}}
    as |progressPercent| >

    {{#if run.topRunAttempt.isExpired}}
      <MyCoursesList::ExpiredCourses::Content
        @progressPercent={{progressPercent}}
        @tagName=''
        @run={{run}} />
    {{else}}
      <MyCoursesList::RecentlyAccessed::Content
        @progressPercent={{progressPercent}}
        @tagName=''
        @run={{run}} />
    {{/if}}

  </MyCoursesList::RunRow>
{{/each}}