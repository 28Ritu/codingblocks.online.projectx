{{#if isErrored}}
  <CodeChallenge::CodeChallengeResult::CodeChallengeResultError @output={{errorPayload}} />
{{else if isSubmission}}
  <CodeChallenge::CodeChallengeResult::CodeChallengeResultTestcases @testcases={{testcasesPayload}} />
{{else}}
  <CodeChallenge::CodeChallengeResult::CodeChallengeResultSuccess @output={{output}} />
{{/if}}
